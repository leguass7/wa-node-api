{
  "name": "wa-node-api",
  "version": "1.0.3",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist",
  "repository": {
    "type": "git",
    "url": "https://github.com/leguass7/wa-node-api.git"
  },
  "author": "Leandro Sbrissa <leandro.sbrissa@hotmail.com>",
  "keywords": [
    "nodejs",
    "whatsapp",
    "api",
    "maxbot",
    "sacdigital"
  ],
  "files": [
    "dist/",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "release": "standard-version --no-verify --skip.tag --release-as patch",
    "lint": "eslint ./src --ext js --ext ts --fix",
    "prebuild": "rimraf dist",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored && yarn build:tsc",
    "build:tsc": "tsc -p tsconfig-build.json --emitDeclarationOnly && tscpaths -p tsconfig-build.json -s ./src -o ./dist",
    "build:2": "yarn prebuild && tsc -p tsconfig-build.json && tscpaths -p tsconfig-build.json -s ./src -o ./dist",
    "start": "node dist/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/dev.ts",
    "test": "jest --coverage"
  },
  "standard-version": {
    "infile": "./CHANGELOG.md"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "camelcase-keys": "^7.0.1",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^27.0.2",
    "@types/valid-url": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "tscpaths": "^0.0.9",
    "typescript": "4.4.3"
  }
}
